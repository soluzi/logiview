{
    "name": "soluzi/logiview",
    "description": "A Laravel package for viewing logs",
    "keywords": [
        "soluzi",
        "logiview"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdelghafour Dadda",
            "email": "contact@abdelghafourdadda.dev",
            "homepage": "https://abdelghafourdadda.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "mockery/mockery": "2.0.x-dev",
        "phpunit/phpunit": "^12.1@dev",
        "symfony/var-dumper": "7.3.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Soluzi\\Logiview\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soluzi\\Logiview\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage --coverage-html",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Soluzi\\LogiView\\LogiViewServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}